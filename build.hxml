-cp src
-main Main

--each
--interp

--next
-js bin/main.js
-cmd node bin/main.js

--next
-D lua-vanilla
-lua bin/main.lua
-cmd lua bin/main.lua

--next
--jvm bin/main.jar
-lib hxjava
-cmd java -jar bin/main.jar

--next
--python bin/main.py
-cmd python bin/main.py

--next
--cs bin/cs
--cmd haxe --run Exec bin/cs/bin Main

--next
-lib hxcpp
--cpp bin/cpp
--cmd haxe --run Exec bin/cpp Main

--next
--hl bin/main.hl
--cmd hl bin/main.hl